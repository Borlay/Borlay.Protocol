<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.6</TargetFramework>
    <Authors>Paulius Kanoverskis</Authors>
    <Copyright>Paulius Kanoverskis</Copyright>
    <PackageLicenseUrl>https://github.com/Borlay/Borlay.Protocol/blob/master/LICENSE.md</PackageLicenseUrl>
    <PackageProjectUrl>http://borlay.org</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Borlay/Borlay.Protocol/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>protocol serialization</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.3</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>F:\github\Borlay.Protocol\Borlay.Protocol\Borlay.Protocol\Borlay.Protocol.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Borlay.Arrays" Version="1.0.0" />
    <PackageReference Include="Borlay.Handling" Version="1.0.14" />
    <PackageReference Include="Borlay.Injection" Version="1.0.10" />
    <PackageReference Include="Borlay.Serialization" Version="1.0.0" />
  </ItemGroup>

</Project>
